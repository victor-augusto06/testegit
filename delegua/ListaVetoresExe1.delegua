var vetorLocais=[]
var vetor=[]
para (var i=0;i<10;i=i+1) {
    var entrada=leia("Escolha um número inteiro")
    vetor.adicionar(inteiro(entrada))
}
escreva(vetor)
para (var i=0;i<10;i=i+1) {
   se (vetor[i]==10) {
    vetorLocais.adicionar(i)
   } 
}
escreva("O numero 10 está presente nos indices "+vetorLocais)